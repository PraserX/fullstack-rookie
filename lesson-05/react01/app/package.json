{
  "name": "rookie-course-application",
  "description": "Rookie course application",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/PraserX/fullstack-rookie.git"
  },
  "main": "server.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config webpack.prod.js && npm run dr:surge",
    "start": "node server.js",
    "start:dev": "webpack-dev-server --hot --color --progress --info=true --config webpack.dev.js",
    "test": "jest",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts}",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.3",
    "@types/victory": "^33.1.4",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "css-loader": "^3.6.0",
    "dotenv-webpack": "^1.8.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.26.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "imagemin": "^7.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.5",
    "prettier": "^2.3.0",
    "prop-types": "^15.6.1",
    "raw-loader": "^4.0.2",
    "react-axe": "^3.5.4",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "style-loader": "^1.3.0",
    "svg-url-loader": "^6.0.0",
    "terser-webpack-plugin": "^3.1.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^7.0.5",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "tslib": "^2.2.0",
    "typescript": "^3.9.9",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@patternfly/react-charts": "^6.14.17",
    "@patternfly/react-core": "^4.115.2",
    "@patternfly/react-icons": "^4.10.2",
    "@patternfly/react-styles": "^4.10.2",
    "@patternfly/react-table": "^4.26.7",
    "@types/dompurify": "^2.2.2",
    "@types/marked": "^1.2.2",
    "dompurify": "^2.2.8",
    "express": "^4.17.1",
    "marked": "^1.2.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-last-location": "^2.0.1"
  }
}
